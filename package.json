  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:docker": "SERVICE_ENV=development && ./scripts/docker-build.sh",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "SERVICE_ENV=local && nest start --watch",
    "start:docker": "yarn build:docker && docker run -d -p 8080:8080 --name minarvas-owl minarvas",
    "start:dev": "SERVICE_ENV=development && nest start --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/server": "4.9.3",
    "@graphql-tools/utils": "10.0.1",
    "@nestjs/apollo": "12.0.3",
